// Finding square root of a number
#include <stdio.h>
int SquareRoot(int number){
    if(number < 0){
        printf("Invalid input. Square root of Negative number is Undefined");
        return -1;
    }
    int num =1;
    while(num * num <= number){
        num ++; 
    }
    return num - 1;
}
int main() {
    int number;
    
    printf("Enter the number:");
    scanf("%d",&number);
    if(SquareRoot(number) != -1){
        printf("The Square Root of %d is %d",number,SquareRoot(number));
    }
    

    return 0;
}
