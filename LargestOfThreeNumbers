// Largest of 3 numbers
#include <stdio.h>


int main(){
    int a,b,c;
    printf("Enter the first value :");
    scanf("%d",&a);
    printf("Enter the secong value :");
    scanf("%d",&b);
    printf("Enter the third value :");
    scanf("%d",&c);
    if(a>b && a>c){
    printf("%d is the largest value",a);
    }else if(b>c){
    printf("%d is the largest value",b);
    }else{
    printf("%d is the largest value",c);
    }
    return 0;

}
